<% if current_user.nil? %>
  <%= render "dashboard/hero" %>
  <%= render "dashboard/call-to-action-banner" %>
<% end %>

<div class="mt-8 flex flex-col px-16 sm:px-10">
  <%= turbo_frame_tag "portfolio_turbo_frame" do %>
  <h3 class="text-4xl font-bold">Markets</h3>
  <p class="text-lg">View by: Trending</p>
    <%= render "shared/filter-table" %>
    <%= render "shared/stocks-container", stocks: @stocks %>
  <% end %>
</div>
